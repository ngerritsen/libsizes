{
  "name": "libsizes",
  "version": "3.1.2",
  "description": "library sizes generated with webpack",
  "main": "lib/index.js",
  "author": "Niels Gerritsen",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngerritsen/libsizes.git"
  },
  "scripts": {
    "start": "node server",
    "build": "parcel build ./client/index.html  -d ./public",
    "serve": "npm run start-containers; concurrently 'parcel watch ./client/index.html -d ./public' 'nodemon server'",
    "test": "eslint .; prettier --check .",
    "fix": "eslint . --fix; prettier --write .",
    "start-containers": "docker-compose up -d",
    "connect-postgres": "docker exec -it libsizes-postgresql sudo -u postgres psql"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.5.0",
    "bluebird": "^3.4.7",
    "eslint-config-prettier": "^8.1.0",
    "express": "^4.17.1",
    "gzip-size": "^3.0.0",
    "lodash": "^4.17.2",
    "mkdirp": "^0.5.1",
    "parcel-bundler": "^1.12.5",
    "pg": "^8.5.1",
    "prettier": "^2.2.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-map-reducers": "^1.0.1",
    "redux-saga": "^1.1.3",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^2.5.4",
    "semver": "^5.3.0",
    "shortid": "^2.2.6",
    "socket.io": "^1.7.2",
    "socket.io-client": "^1.7.2",
    "webpack": "^5.27.1",
    "winston": "^2.3.0"
  },
  "devDependencies": {
    "concurrently": "^6.0.0",
    "eslint": "^7.22.0",
    "eslint-plugin-react": "^7.22.0",
    "nodemon": "^2.0.7",
    "redux-logger": "^3.0.6",
    "sass": "^1.32.8"
  },
  "engines": {
    "node": ">10"
  },
  "keywords": [
    "library",
    "filesize",
    "javascript",
    "webpack"
  ],
  "pre-commit": [
    "test"
  ],
  "bugs": {
    "url": "https://github.com/ngerritsen/libsizes/issues"
  },
  "homepage": "https://github.com/ngerritsen/libsizes#readme"
}
